<UserControl x:Class="FiveElementsIntTest.CompImage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="166" d:DesignWidth="166" BorderBrush="White" Background="Black" BorderThickness="0">
    <Grid>
        <Image Height="166" HorizontalAlignment="Left" Name="amImage" Stretch="Fill" VerticalAlignment="Top" Width="166" />
        <Border BorderBrush="White" BorderThickness="2" Height="166" HorizontalAlignment="Left" Name="amBorder" VerticalAlignment="Top" Width="166" Background="{x:Null}" />
    </Grid>
</UserControl>

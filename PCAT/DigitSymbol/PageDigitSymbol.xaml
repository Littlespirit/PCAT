<Page x:Class="FiveElementsIntTest.PageDigitSymbol"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
	Title="PageDigitSymbol" Loaded="Page_Loaded" Focusable="True" KeyUp="Page_KeyUp">

    <Grid Focusable="False">
        <Canvas Height="100" HorizontalAlignment="Left" Name="mBaseCanvas" VerticalAlignment="Top" Width="200" Background="Black">
            <Border BorderBrush="#FF787878" BorderThickness="1" Canvas.Left="72" Canvas.Top="65" Height="600" Name="mAuxBorder" Width="800" Focusable="False" />
            <Border BorderBrush="#FF787878" BorderThickness="1" Focusable="False" Height="768" Name="mAuxBorder1024" Width="1024" />
        </Canvas>
    </Grid>
</Page>
